# Aaroth Fresh Frontend - Production Environment Variables

# API Configuration (Replace with actual production URL)
VITE_API_BASE_URL=https://api.aarothfresh.com/api/v1
VITE_API_TIMEOUT=15000

# App Configuration
VITE_APP_NAME=Aaroth Fresh
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=B2B Fresh Produce Marketplace

# Feature Flags
VITE_ENABLE_DEV_TOOLS=false
VITE_ENABLE_MOCK_API=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Authentication
VITE_TOKEN_STORAGE_KEY=token
VITE_SESSION_TIMEOUT=24

# File Upload
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/webp

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ITEMS_PER_PAGE=20
VITE_DEBOUNCE_DELAY=300

# Production Specific
VITE_SHOW_DEBUG_INFO=false
VITE_LOG_LEVEL=error
VITE_SENTRY_DSN=
VITE_GA_TRACKING_ID=